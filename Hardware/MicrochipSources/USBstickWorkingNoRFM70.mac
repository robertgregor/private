LOOKDOWN?BCLB macro Bin, Cin, Label, Bout
    local loop, getval, valtbl
        MOVE?CB -1, R1
loop    incf    R1, F
        movf    R1, W
        sublw   (Cin) - 1
        BIT?GOTO 0, STATUS, C, Label
        L?CALL  getval
        CHK?RP  Bin
        subwf   Bin, W
        BIT?GOTO 0, STATUS, Z, loop
        MOVE?BB R1, Bout
        L?GOTO  Label
getval  movlw   high (valtbl)
        movwf   PCLATH
        movf    R1, W
	addlw	low (valtbl)
	btfsc	STATUS, C
	incf	PCLATH, F
	movwf	PCL
valtbl
    endm

LDRET?C macro Cin
