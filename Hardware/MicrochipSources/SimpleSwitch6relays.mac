AIN?BBB macro Bin, Bout, Bndx
        CHK?RP  Bndx
        movf    Bndx, W
        addlw   low (Bout)
        movwf   FSR
    if (Bout > 255)
        bsf     STATUS, IRP
    endif
        CHK?RP  Bin
        movf    Bin, W
        movwf   INDF
    if (Bout > 255)
        bcf     STATUS, IRP
    endif
    endm

