    $( "#configureCOMPONENTID" ).button().click(function() {
          var nm = document.getElementById("nameFormCOMPONENTID").value;
          if (nm == '') {
                alert(error_name_cannot_be_empty);
                return;
          }
          var tm = parseInt(document.getElementById("currPeriodFormCOMPONENTID").value);
          if (isNaN(tm)) {
              alert(error_period_exist);
              return;
          }
          if ((tm > 2550) || (tm < 10)) {
              alert(error_period_range);
              return;
          }
          var alEnterTimeout = parseInt(document.getElementById("alEnterTimeoutFormCOMPONENTID").value);
          if (isNaN(alEnterTimeout)) {
            alert(err_enter_number);
            return;
          }
          var alLeaveTimeout = parseInt(document.getElementById("alLeaveTimeoutFormCOMPONENTID").value);
          if (isNaN(alLeaveTimeout)) {
            alert(err_leave_number);
            return;
          }
          var alTimeout = parseInt(document.getElementById("alTimeoutFormCOMPONENTID").value);
          if (isNaN(alTimeout)) {
            alert(err_timeout_number);
            return;
          }
          var room = document.getElementById("roomFormCOMPONENTID").value;
          if (room == '') {
              alert(error_room_exist);
              return;
          }
          var swDeviceId = parseInt(document.getElementById("swDeviceIdFormCOMPONENTID").value);
          if (isNaN(swDeviceId)) {
            alert(err_swDeviceId_number);
            return;
