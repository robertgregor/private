              alert(err_alSubDeviceId_number);
              return;
          }
          var alschenabled = document.getElementById("checkboxEnabledAlSchedulerCOMPONENTID").checked?"TRUE":"FALSE";
          var swschenabled = document.getElementById("checkboxEnabledSwSchedulerCOMPONENTID").checked?"TRUE":"FALSE";
          var emailFirstTimeEnabled = document.getElementById("checkboxEnabledEmailCOMPONENTID").checked?"TRUE":"FALSE";
          var emailEveryTimeEnabled = document.getElementById("checkboxEnabledEmailEveryTimeCOMPONENTID").checked?"TRUE":"FALSE";
