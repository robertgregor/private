          var smtpSubjectForm = document.getElementById("smtpSubjectFormCOMPONENTID").value;
          var smtpMessageForm = document.getElementById("smtpMessageFormCOMPONENTID").value;
          $.ajax({
              url: "/",
              type: "get",
              'cache' : false,
              data: "ServiceName=MagneticSensorDeviceManager&deviceId=COMPONENTID&tm="+tm+"&swDeviceId="+swDeviceId+"&swSubDeviceId="+swSubDeviceId+"&alDeviceId="+alDeviceId+"&alSubDeviceId="+alSubDeviceId+"&alEnterTimeout="+alEnterTimeout+"&alLeaveTimeout="+alLeaveTimeout+"&alTimeout="+alTimeout+"&room="+encodeURIComponent(room)+"&nm="+encodeURIComponent(nm)+"&smtpToForm="+encodeURIComponent(smtpToForm)+"&smtpSubjectForm="+encodeURIComponent(smtpSubjectForm)+"&smtpMessageForm="+encodeURIComponent(smtpMessageForm)+"&alschenabled="+alschenabled+"&swschenabled="+swschenabled+"&emailFirstTimeEnabled="+emailFirstTimeEnabled+"&emailEveryTimeEnabled="+emailEveryTimeEnabled+"&action=CONFIGURE",
