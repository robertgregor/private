function updateStatusCOMPONENTID(showError) {
  $.ajax({
      url: "/",
      type: "get",
      'cache' : false,
      data: "ServiceName=GetJsonObject&deviceId=COMPONENTID",
      success: function(indata) {
          if (indata.indexOf("<!--LOGIN-->") > 0) { location.replace("/"); return; }
          var data = JSON.parse(indata);
          document.getElementById("nameCOMPONENTID").innerHTML=data.deviceName;
          document.getElementById("nameFormCOMPONENTID").value=data.deviceName;
          document.getElementById("roomCOMPONENTID").innerHTML=data.roomName;
          document.getElementById("roomFormCOMPONENTID").value=data.roomName;
          document.getElementById("compIdCOMPONENTID").innerHTML=data.deviceId;
          document.getElementById("currPeriodCOMPONENTID").innerHTML=data.frequency;
          document.getElementById("currPeriodFormCOMPONENTID").value=data.frequency;
          document.getElementById("battVoltageCOMPONENTID").innerHTML=data.battery;
          document.getElementById("alEnterTimeoutCOMPONENTID").innerHTML=data.alarmEnterTimeout;
          document.getElementById("alLeaveTimeoutCOMPONENTID").innerHTML=data.alarmLeaveTimeout;
          document.getElementById("alTimeoutCOMPONENTID").innerHTML=data.alarmTimeout;
          document.getElementById("alEnterTimeoutFormCOMPONENTID").value=data.alarmEnterTimeout;
          document.getElementById("alLeaveTimeoutFormCOMPONENTID").value=data.alarmLeaveTimeout;
          document.getElementById("alTimeoutFormCOMPONENTID").value=data.alarmTimeout;
          document.getElementById("swDeviceIdCOMPONENTID").innerHTML=data.switchDeviceId;
          document.getElementById("swDeviceIdFormCOMPONENTID").value=data.switchDeviceId;
          document.getElementById("swSubDeviceIdCOMPONENTID").innerHTML=data.switchSubDeviceId;
          document.getElementById("swSubDeviceIdFormCOMPONENTID").value=data.switchSubDeviceId;
          document.getElementById("alDeviceIdCOMPONENTID").innerHTML=data.alarmDeviceId;
          document.getElementById("alDeviceIdFormCOMPONENTID").value=data.alarmDeviceId;
          document.getElementById("alSubDeviceIdCOMPONENTID").innerHTML=data.alarmSubDeviceId;
          document.getElementById("alSubDeviceIdFormCOMPONENTID").value=data.alarmSubDeviceId;
          if (data.magnetState == 1) {
              document.getElementById("sensorStateCOMPONENTID").innerHTML=magnetic_sensor_state_open;
          } else if (data.magnetState == 0) {
              document.getElementById("sensorStateCOMPONENTID").innerHTML=magnetic_sensor_state_close;
          } else {
              document.getElementById("sensorStateCOMPONENTID").innerHTML=magnetic_sensor_state_unknown;
          }
          if (data.manageFrequencyAuto) {
            document.getElementById("expPeriodCOMPONENTID").innerHTML=data.expectedFrequency;
            setLabelBasedOnId("secondsL1COMPONENTID", "seconds");
          } else {
            document.getElementById("expPeriodCOMPONENTID").innerHTML="";
            document.getElementById("secondsL1COMPONENTID").innerHTML="";
          }
          if (data.enabledAlarmScheduler) {
                $( "#checkboxEnabledAlSchedulerCOMPONENTID" ).prop('checked', true);
                document.getElementById("alEnabledSchedulerCOMPONENTID").innerHTML=yes;
          } else {
                $( "#checkboxEnabledAlSchedulerCOMPONENTID" ).prop('checked', false);
                document.getElementById("alEnabledSchedulerCOMPONENTID").innerHTML=no;
          }
          $("#checkboxEnabledAlSchedulerCOMPONENTID").trigger("change");
          if (data.enabledSwitchScheduler) {
                $( "#checkboxEnabledSwSchedulerCOMPONENTID" ).prop('checked', true);
                document.getElementById("swEnabledSchedulerCOMPONENTID").innerHTML=yes;
          } else {
                $( "#checkboxEnabledSwSchedulerCOMPONENTID" ).prop('checked', false);
                document.getElementById("swEnabledSchedulerCOMPONENTID").innerHTML=no;
          }
          $("#checkboxEnabledSwSchedulerCOMPONENTID").trigger("change");
          if (data.enabled) {
                document.getElementById("sensorEnabledCOMPONENTID").innerHTML=yes;
          } else {
                document.getElementById("sensorEnabledCOMPONENTID").innerHTML=no;
          }
          if (data.alarmEnabled) {
                document.getElementById("sensorAlEnabledCOMPONENTID").innerHTML=yes;
          } else {
                document.getElementById("sensorAlEnabledCOMPONENTID").innerHTML=no;
          }
          if (data.switchEnabled) {
                document.getElementById("sensorSwEnabledCOMPONENTID").innerHTML=yes;
          } else {
                document.getElementById("sensorSwEnabledCOMPONENTID").innerHTML=no;
          }
          if (data.manageEnabledAuto) {
                if (data.expectedEnabled) {
                    document.getElementById("sensorExpectedEnabledCOMPONENTID").innerHTML=yes;
                } else {
                    document.getElementById("sensorExpectedEnabledCOMPONENTID").innerHTML=no;
                }
          } else {
            document.getElementById("sensorExpectedEnabledCOMPONENTID").innerHTML="";
          }
          if (data.intruded) {
                document.getElementById("sensorAlarmReportedCOMPONENTID").innerHTML=yes;
          } else {
                document.getElementById("sensorAlarmReportedCOMPONENTID").innerHTML=no;
          }
          var labelEmailReport = new Array(no, no);
          if (data.emailAlertFirstTime) {
                $( "#checkboxEnabledEmailCOMPONENTID" ).prop('checked', true);
                labelEmailReport[0] = yes;
          } else {
                $( "#checkboxEnabledEmailCOMPONENTID" ).prop('checked', false);
          }
          $("#checkboxEnabledEmailCOMPONENTID").trigger("change");
          if (data.emailAlertEveryTime) {
                $( "#checkboxEnabledEmailEveryTimeCOMPONENTID" ).prop('checked', true);
                labelEmailReport[1] = yes;
          } else {
                $( "#checkboxEnabledEmailEveryTimeCOMPONENTID" ).prop('checked', false);
          }
          $("#checkboxEnabledEmailEveryTimeCOMPONENTID").trigger("change");
          document.getElementById("sensorEmailReportingEnabledCOMPONENTID").innerHTML = labelEmailReport[0] + "&nbsp;|&nbsp;" + labelEmailReport[1];
          document.getElementById("smtpToCOMPONENTID").innerHTML=data.smtpTo;
          document.getElementById("smtpToFormCOMPONENTID").value=data.smtpTo;
          document.getElementById("smtpSubjectCOMPONENTID").innerHTML=data.smtpSubject;
          document.getElementById("smtpSubjectFormCOMPONENTID").value=data.smtpSubject;
          document.getElementById("smtpMessageCOMPONENTID").innerHTML=data.smtpMessage;
          document.getElementById("smtpMessageFormCOMPONENTID").value=data.smtpMessage;
          document.getElementById("timestampCOMPONENTID").innerHTML=new Date(data.timestamp).europeDate();
      },
      error:function(xhr){
          if (showError) {
                alert(error_get_device_status+"\n\n"+xhr.responseText);
          }
      }
  });
}
